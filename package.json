{
  "name": "phone_catalog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier . --ignore-path .gitignore --write",
    "fix-style": "npm run format & npm run lint",
    "deploy": "npm run build && mate-scripts deploy"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "canvas-confetti": "^1.9.2",
    "classnames": "^2.5.1",
    "framer-motion": "^11.0.6",
    "hamburger-react": "^2.5.0",
    "lodash": "^4.17.21",
    "postcss-custom-properties": "^13.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.0.6",
    "usehooks-ts": "^2.15.1"
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "^1.0.15",
    "@mate-academy/scripts": "^1.7.0",
    "@types/canvas-confetti": "^1.6.4",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-vue": "^9.21.1",
    "gh-pages": "^3.2.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.71.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  },
  "lint-staged": {
    "*.js, *.ts, *.tsx": "npm run fix-style"
  },
  "mateAcademy": {
    "_comment": "Replace 'reactTypescript' with 'react' if you want use React without Typescript",
    "projectType": "reactTypescript"
  }
}
